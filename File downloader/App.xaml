<Application x:Class="File_downloader.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:File_downloader"
             StartupUri="Views/MainWindow.xaml">
    
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/ResourceAccess/BrushesDictionary.xaml"/>
                <ResourceDictionary Source="/Resources/ResourceAccess/AssetsDictionary.xaml"/>
                <ResourceDictionary Source="/Resources/ResourceAccess/StringDictionary.xaml"/>
                <ResourceDictionary Source="/Resources/ResourceAccess/StyleDictionary.xaml"/>
                <ResourceDictionary Source="/Resources/ResourceAccess/ConvertersDictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
    
</Application>
