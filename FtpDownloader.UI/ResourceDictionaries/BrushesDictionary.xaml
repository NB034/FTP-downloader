<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="MainBorderBrush" Color="Black"/>
    <SolidColorBrush x:Key="MainBackgroundBrush" Color="White"/>
    <SolidColorBrush x:Key="AdditionalBackgroundBrush" Color="LightBlue"/>
    <SolidColorBrush x:Key="UnavailableElementBrush" Color="LightGray"/>
    <SolidColorBrush x:Key="BlockBrush" Color="#CCD3D3D3"/>
    <SolidColorBrush x:Key="TagBackGroundBrush" Color="WhiteSmoke"/>
    <SolidColorBrush x:Key="ErrorBrush" Color="Red"/>

</ResourceDictionary>